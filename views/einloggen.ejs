

<% include layout %>

<div id="einloggen">


<form method="POST" action="/einloggen">
<br />
<div class="row">
        <div class="col-lg-6">
                <div class="input-group">
                    <span class="input-group-addon">Benutzer Name </span>
                    <input type="text" class="form-control" placeholder="">
                </div>
        </div>
</div>
<br />
<div class="row">
        <div class="col-lg-6">
                <div class="input-group">
                    <span class="input-group-addon">Pass Phrase &nbsp; &nbsp; </span>
                    <input type="password" class="form-control" placeholder="">
                </div>
        </div>
</div>
<br />
<div class="row">
        <div class="col-lg-6">
                <div class="input-group">
                    <button type="submit" class="btn btn-primary">Einloggen</button>
                    <button type="button" class="btn btn-warning">Registrieren</button>
                    <button type="button" class="btn btn-danger">Exit &nbsp;  &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;        </button>
                </div>
        </div>
</div>
<br />
<br />

</form>

</div>
    
